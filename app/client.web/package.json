  "scripts": {
    "start": "yarn run start:app",
    "start:app": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config=webpack.config.js --env.mode=development --env.source=client --env.sourceMap=true",
    "start:rdr": "echo WIP",
    "start:srv": "ts-node --project tsconfig.server.json ./src/server/main.ts",
    "test": "yarn run test:app",
    "test:app": "cross-env NODE_ENV=test jest --logHeapUsage --forceExit --detectOpenHandles",
    "test:app:watch": "cross-env NODE_ENV=test jest --logHeapUsage --forceExit --detectOpenHandles --watch",
    "test:app:snapshot": "cross-env NODE_ENV=test jest --updateSnapshot --logHeapUsage --forceExit --detectOpenHandles",
    "test:app:coverage": "cross-env NODE_ENV=test jest --logHeapUsage --forceExit --detectOpenHandles --collectCoverage && yarn run gen-readme",
    "test:rdr": "echo WIP",
    "test:srv": "echo WIP",
    "build": "node_modules/.bin/gulp",
    "build:app": "node_modules/.bin/gulp buildClient",
    "build:rdr": "node_modules/.bin/gulp buildRenderer",
    "build:srv": "node_modules/.bin/gulp buildServer",
    "gen-proto": "node_modules/.bin/gulp generateProto",
    "gen-readme": "node_modules/.bin/jest-badges-readme --coverageDir='./.coverage'"
  },
