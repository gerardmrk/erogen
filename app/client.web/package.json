{
  "name": "client.web",
  "version": "1.0.0",
  "description": "web app",
  "scripts": {
    "start": "npm run app:start",
    "build": "npm run app:build",
    "app:start": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config=config/webpack.config.js --env.mode=development --env.source=client",
    "app:build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config=config/webpack.config.js --env.mode=production --env.source=client",
    "app:test": "cross-env NODE_ENV=test jest --config=./config/jest.config.js --logHeapUsage --forceExit --detectOpenHandles",
    "rdr:start": "",
    "rdr:build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config=config/webpack.config.js --env.mode=production --env.source=renderer",
    "rdr:proto:js": "./node_modules/.bin/pbjs -t static-module -w commonjs -o ./src/renderer/proto/index.js ./src/renderer.proto; ./node_modules/.bin/pbts -o ./src/renderer/proto/index.d.ts ./src/renderer/proto/index.js",
    "rdr:proto:go": "protoc --go_out=./src/server/internal ./src/renderer.proto",
    "srv:build": "tsc -p tsconfig.build.json",
    "srv:format": "prettier --write \"src/**/*.ts\"",
    "srv:start": "ts-node --project ./tsconfig.server.json -r tsconfig-paths/register src/server/main.ts",
    "srv:start:dev": "nodemon",
    "srv:start:debug": "nodemon --config nodemon-debug.json",
    "srv:prestart:prod": "rimraf dist && npm run build",
    "srv:start:prod": "node dist/server/main.js",
    "srv:lint": "tslint -p tsconfig.json -c tslint.json",
    "srv:test": "jest",
    "srv:test:watch": "jest --watch",
    "srv:test:cov": "jest --coverage",
    "srv:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "srv:test:e2e": "jest --config ./test/jest-e2e.json",
    "tmp:rserver": "cross-env DEBUG=* node ./dist/renderer/index.js --mode uds --addr /tmp/server.sock --statsfile",
    "tmp:rclient": "node ./tmp.rclient.json.js"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
